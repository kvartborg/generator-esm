  },
  "devDependencies": {
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "jest": "^21.1.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.7.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
